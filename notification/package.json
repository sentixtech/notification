{
    "name": "laravel-alert-notification",
    "version": "1.0.0",
    "description": "Laravel Alert Notification Plugin with configurable positions and colors",
    "main": "resources/js/app.js",
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production"
    },
    "devDependencies": {
        "laravel-mix": "^6.0.49",
        "sass": "^1.56.0",
        "sass-loader": "^13.0.2"
    },
    "dependencies": {
        "bootstrap": "^5.2.0"
    }
}
