{
    "name": "sentix/notification",
    "description": "Laravel Alert Notification Plugin with configurable positions and colors",
    "type": "library",
    "license": "MIT",
    "version": "1.0.1",
    "authors": [
        {
            "name": "Vikas Kumar",
            "email": "sentixtech@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/view": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Alert\\": "src/Alert/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alert\\AlertNotificationServiceProvider"
            ],
            "aliases": {
                "Alert": "Alert\\Facades\\Alert"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}